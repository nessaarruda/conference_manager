<nav>

  <%= link_to "Conferences", "/conferences" %>
  <%= link_to "Presentations", "/presentations" %>
  <%= link_to "Meeting Rooms", "/meeting_rooms" %>
  <%= link_to "Meetings", "/meetings" %>

</nav>

<h1>New Meeting</h1>

<form action='/meetings' method='post'>
  <input type='hidden' name='authenticity_token' value="<%= form_authenticity_token %>">
  <input type='hidden' name='meeting_room_id' value="<%= params[:id] %>">
  <label for='mename'>Meeting name: </label>
  <input id='mename' type='text' name='meeting[name]'/><br/>
  <label for='number_of_participants'>Number of Participants: </label>
  <input id='number_of_participants' type='bigint' name='meeting[number_of_participants]'/><br/>
  <label for='mestart_time'>Start Time: </label>
  <input id='mestart_time' type='datetime-local' name='meeting[start_time]'/><br/>
  <label for='end_time'>End Time: </label>
  <input id='end_time' type='datetime-local' name='meeting[end_time]'/><br/>
  <input type='submit' value='Create Meeting'/>
</form>
