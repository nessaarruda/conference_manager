<nav>
  <%= link_to "Conference Manager Home", '/' %>
  <%= link_to "Conferences", "/conferences" %>
  <%= link_to "Presentations", "/presentations" %>
  <%= link_to "Meeting Rooms", "/meeting_rooms" %>
  <%= link_to "Meetings", "/meetings" %>
</nav>

<h1>New Meeting</h1>

  <%= form_tag "/meeting_rooms/#{params[:id]}/meetings" do %>
  <%= hidden_field_tag :meeting_room_id, params[:id] %>
  <%= label_tag "Meeting name: " %>
  <%= text_field_tag :name %><br>
  <%= label_tag :number_of_participants %>
  <%= number_field_tag :number_of_participants %><br>
  <%= label_tag :start_time  %>
  <%= datetime_field :meeting, :start_time  %><br>
  <%= label_tag :end_time %>
  <%= datetime_field :meeting, :end_time  %><br><br>
  <%= submit_tag "Create Meeting"  %>
<% end %>
