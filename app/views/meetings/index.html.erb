<h1>Meetings</h1>
<% @meetings.each do |meeting| %>

  <h3><a href="/meetings/<%= meeting.id %>"><%= meeting.name %></a></h3>
  <h3><a href="/meetings/<%= meeting.id %>"><%= meeting.schedule %></a></h3>
  <h3><a href="/meetings/<%= meeting.id %>"><%= meeting.number_of_participants %></a></h3>
  <h3><a href="/meetings/<%= meeting.id %>"><%= meeting.duration %></a></h3>
<% end %>
<br>
<a href='/meeting_rooms/new'>New Meeting</a>
