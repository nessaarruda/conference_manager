<nav>
  <%= link_to "Conference Manager Home", '/' %>
  <%= link_to "Conferences", "/conferences" %>
  <%= link_to "Presentations", "/presentations" %>
  <%= link_to "Meeting Rooms", "/meeting_rooms" %>
  <%= link_to "Meetings", "/meetings" %>
</nav>

<h1>Update Meeting Room </h1>

<%= form_tag "/meeting_rooms/#{@meeting_room.id}", method: :patch do %>
  <%= hidden_field_tag :previous_request, request.referrer %>
  <%= label_tag "Meeting room name" %>
  <%= text_field_tag :name, @meeting_room.name %><br>
  <%= label_tag :capacity %>
  <%= number_field_tag :capacity, @meeting_room.capacity %><br>
  <%= label_tag :has_projector %>
  <%= hidden_field_tag :has_projector, false %>
  <%= check_box_tag :has_projector, true, @meeting_room.has_projector %><br>
  <%= submit_tag "Update Meeting Room" %>
<% end %>
